// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id          String   @id @default(cuid())
  slug        String   @unique
  studentName String
  originalUrl String
  createdAt   DateTime @default(now())
}